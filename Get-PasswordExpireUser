# Get-PasswordExpireUser in 7 days
Get-ADUser -Filter 'PasswordExpired -eq $False -and PasswordNeverExpires -eq $False' -Properties * `
| Where-Object {$_.PasswordLastSet.AddDays(90) -gt (Get-Date) -and $_.PasswordLastSet.AddDays(90) -lt (Get-Date).AddDays(7)} `
| Select-Object SamAccountName,Name,PasswordLastSet,@{Name='PasswordExpire';Expression={$_.PasswordLastSet.AddDays(90)}}
